{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "QPConnectionString": "connection to QP database",
    "Type": "Postgres"
    //"Type": "SqlServer"
  },
  //"ContentIds": [ 30745, 30746, 30747 ],
  "QpArticlesAccessor": {
    "CalculatePagingData": true
  },

  "GraphQLRouting": {
    "BasePath": "/graphql",
    "States": {
      "Live": {
        "Allow": true,
        "Default": true
      },
      "Stage": {
        "Allow": true
      }
    }
  },

  "SchemaAutoReload": true,
  "SchemaReloadInterval": "00:02:00",
  "EnableMetrics": true,
  "ExposeExceptions": true,

  "QpPluginSettings": {
    "ContractVersion": "0.5.0"
  }
}
